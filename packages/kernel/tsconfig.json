{
  "compilerOptions": {
    "lib": ["es2023", "ESNext.Collection"],
    "types": ["node"],
    "noEmit": true,
    "allowImportingTsExtensions": true,
    "target": "ES2023",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
